    <div class="container">
      <h1 style="color:red"><%= error %></h1>

      <div class="starter-template">
        <form method="GET" action="/">
          <div class="form-group">
            <label for="exampleInputEmail1">Bootstrap CSS URL</label>
            <input type="text" name="cssURL" class="form-control" id="exampleInputEmail1"  value="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.css">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Namespace</label>
            <input type="text" name="ns" class="form-control" id="exampleInputPassword1" value="ns">
          </div>

          <button type="submit" class="btn btn-default">Submit</button>
        </form>
      </div>
      </div><!-- /.container -->
